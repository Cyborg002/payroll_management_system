CONNECT salary_mgmt/dbase@XE;
CREATE OR REPLACE PACKAGE report
AS
  PROCEDURE payslip(
      p_employee_id IN NUMBER,
      p_month       IN NUMBER,
      p_year        IN NUMBER,
      p_lencash     IN VARCHAR2);
  PROCEDURE payroll(
      p_month IN NUMBER,
      p_year  IN NUMBER);
  PROCEDURE salary_structure(
      p_employee_id IN NUMBER,
      p_year        IN NUMBER);
  PROCEDURE leave_report(
      p_employee_id IN NUMBER);
END;
/
